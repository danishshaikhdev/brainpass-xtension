{
  "name": "BrainPass",
  "description": "Block distracting websites and boost your productivity with BrainPass.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "webRequest", "cookies"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "public/popup.html"
  },
  "background": {
    "service_worker": "src/background.js"
  }
}
